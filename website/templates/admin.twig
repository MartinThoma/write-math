{% extends "base.twig" %}

{% block content %}
<h2>Flags</h2>
<ul>
    {% for flag in flags %}
        <li><a href="../view/?raw_data_id={{ flag.raw_data_id }}">{{ flag.raw_data_id }}</a> 
            by <a href="../user/?id={{ flag.user_id }}">{{ flag.display_name }}</a>
        </li>
    {% endfor %}
</ul>

<h2>Many lines</h2>
<ul>
    {% for drawing in many_lines %}
        <li>Lines: <a href="../view/?raw_data_id={{ drawing.id }}">{{ drawing.lines }}</a></li>
    {% endfor %}
</ul>

{% endblock %}